[filter-app:main]
use = egg:fanstatic#fanstatic
recompute_hashes = false
versioning = true
bottom = true
minified = true
next = ${ppackage} 

[filter:locale]
use = egg:cromlech.i18n#locale

[app:${ppackage}]
use = egg:${ppackage}#app
name = ${ppackage}
zcml_file = $${zcml:output}

[server:main]
use = egg:Paste#http
host = 0.0.0.0
port = 8080

